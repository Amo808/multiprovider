# Production environment settings for Render
# These will override default settings when deployed

# Dev mode settings (bypass Google Auth in production)
DEV_MODE=1
FORCE_DEV_AUTH=1
BYPASS_GOOGLE_AUTH=1

# Frontend dev mode
VITE_DEV_MODE=1
VITE_GOOGLE_CLIENT_ID=

# Server settings
PORT=10000
HOST=0.0.0.0
PYTHONUNBUFFERED=1
DEBUG=False

# JWT settings (still needed for dev auth)
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES=1440

# Optional: Add your API keys here or set them in Render dashboard
# OPENAI_API_KEY=your-openai-key
# ANTHROPIC_API_KEY=your-anthropic-key
# DEEPSEEK_API_KEY=your-deepseek-key
